{
  "manifest_version": 2,
  "name": "Super Highlight Search",
  "description": "検索文字を超ハイライト表示／検索結果サマリから追跡／IME変換途中検索／全角かな↔カナ、英数字記号全角↔半角自動変換検索／正規表現(Regular Expression)検索／FRAMESET対応",
  "version": "0.3.11",
  "default_locale": "en",
  "browser_action": {
    "default_title": "Super Hilight Search",
    "default_icon": "images/sh128_7.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/sh128_7.png"
  },
  "content_security_policy": "script-src 'unsafe-eval' 'self' 'sha256-338befbbc933e3278fdce26af7247b80babb68e67f0864710cf5531a915cd972'; object-src 'self';",
  "permissions": ["*://*/*"]
}